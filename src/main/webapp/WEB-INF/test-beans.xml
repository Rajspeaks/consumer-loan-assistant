<?xml version="1.0" encoding="UTF-8"?>
<beans
  xmlns="http://www.springframework.org/schema/beans"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xmlns:p="http://www.springframework.org/schema/p"
  xsi:schemaLocation="
	   http://www.springframework.org/schema/beans 
	   http://www.springframework.org/schema/beans/spring-beans.xsd">

  <bean
    id="exampleLoanForm"
    class="com.westbank.web.form.ApplicationForm"
    p:loanAmount="100000"
    p:loanReason="Home Mortgage"
    p:loanTerm="8"
    p:totalPurchasePrice="120000"
    p:personalCapitalContribution="20000"
    p:interestRate="0.04"
    p:residenceType="MAIN_HOUSE"
    p:estateType="PARKING"
    p:estateAddress="123 Green Str., Mount Waverley, VIC 3149"
    p:startDate="01/01/2011"

    p:borrowerTitle="Miss"
    p:borrowerFirstName="Alice"
    p:borrowerLastName="Power"
    p:borrowerCity="Melbourne"
    p:borrowerState="Victoria"
    p:borrowerCountry="Australia"
    p:borrowerEmail="power@abc.com"
    p:borrowerMaritalStatus="MARRIED"
    p:borrowerIncome="48000"
    p:borrowerLengthOfService="6"
    p:borrowerPhone="012345678"
    p:borrowerMobilePhone="06501234567"
    p:borrowerStreet="3 Queen Road"
    p:borrowerZipcode="3031"
    p:borrowerOccupation="Accountant"
    p:borrowerDateOfBirth="01/01/1988"
    
    p:hasCoborrower="false"
    p:coborrowerTitle="Mr"
    p:coborrowerFirstName="Lewis"
    p:coborrowerLastName="Schneider"
    p:coborrowerIncome="70000"
    p:coborrowerLengthOfService="8"
    p:coborrowerOccupation="Project Manager"
    p:coborrowerDateOfBirth="01/01/1982"
    p:coborrowerEmail="schneider@free.org" />

</beans>
